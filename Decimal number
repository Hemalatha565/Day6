#include <stdio.h>
int find(int decimal_number)
{
    if (decimal_number == 0)
        return 0;
    else
        return (decimal_number % 2 + 10 *
                find(decimal_number / 2));
}
int main()
{
    int a;
    int decimal_number = a;
    printf(" a:");
    scanf("%d",&a);
    printf("%d", find(a));
return 0;
}
